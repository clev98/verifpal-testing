attacker [active]

principal Alice[
	knows private a
	generates b
	ha = HASH(a)
	hb = HASH(b)
]

Alice -> Bob: ha, hb

principal Bob[
	knows private a
	_ = ASSERT(ha, HASH(a))
]

queries[
	freshness? ha
	freshness? hb
]